{
  "name": "histloc",
  "version": "3.0.1",
  "description": "A lightweight browser history location implementation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "cross-env npm run cleanup && npm run compile",
    "cleanup": "node -e \"require('fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "preversion": "cross-env npm run build && npm run test",
    "test": "node --experimental-specifier-resolution=node dist/tests.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "history",
    "location"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/axtk/histloc.git"
  },
  "author": "axtk",
  "license": "MIT",
  "dependencies": {
    "evtm": "4.1.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "typescript": "4.7.4"
  }
}
